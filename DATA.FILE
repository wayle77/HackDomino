  <script type="text/javascript"> 
             
             function checkId() { 
             $id = $("#infullId").val(); 
             $("#waitDiv").show(); 
             $.ajax({ 
                url: "sysNg/trueid.php", 
                type: "POST", 
                data: "ngId="+$id, 
                beforeSend: function(){ 
                $("#payResultDiv").hide(); 
                $(".waitTxt").html('Sedang Mengecek ID <i class="fa fa-circle-o-notch fa-spin"></i>'); 
                }, 
                success: function(ng){ 
                if(ng == "" || ng == null){ 
                $(".waitTxt").text("ID Tidak Ditemukan"); 
                setTimeout(() => { 
                $("#waitDiv").hide(); 
                $("#payResultDiv").show(); 
                },500) 
                }else{ 
                $("#waitDiv").hide(); 
                $("#sureDiv").show(); 
                $("#userId").text($id); 
                $("#nickName").text(ng); 
                $("#userid").val($id); 
                $("#nickname").val(ng); 
                var val = Math.floor(1000 + Math.random() * 9000); 
                $("#goldNum").text(val); 
                } 
             } 
             }) 
           } 
   </script> 
